#include <stdio.h>
int main (){
    int a,b,c,d,e;
    int flag = 1;
    scanf ("%d%d%d",&b,&c,&d);
    if ((c-b)==(d-c))
        flag = 0;
    if (flag){
        a = b/(c/b);
        e = d*(c/b);
    }
    else{
        a = b-(c-b);
        e = d+(c-b);
    }
    printf("%d %d",a,e);
    return 0;
}