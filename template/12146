#include <stdio.h>
int a[100005];
int b[100005];
int c[100005];
int n,k;
int compare()
long long int sum;
int main(){
  scanf("%d%d", &n, &k);
  for(int i=0;i<n;i++)
    scanf("%d", &a[i]);
  for(int i=0;i<n;i++){
    scanf("%d", &b[i]);
    c[i] = b[i]-a[i];
    if(c[i] >= 0){
      sum += a[i];
      k--;
    }
  }
  
}