#include <stdio.h>
int main(){
    int m,n;
    long long int min = 1e10;
    scanf("%d%d", &m, &n);
    for(int i=0;i<n;i++){
        for(int i=0;i<m;i++){
            long long int now;
            scanf("%lld", &now);
            if(min > now)
                min = now;
        }
    }
    printf("%lld\n", min);
}